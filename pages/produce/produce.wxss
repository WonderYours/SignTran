page{
  background-color: #f5f5f5;
}

.music{
  top: 213rpx;
  position: fixed;
  left: 3%;
}
.cu-progress{
  height: 6rpx;
  top:200rpx;
  width: 80%;
  float: left;
  background-color: rgba(83, 83, 83, 0.2);
  margin: 5% 5% 0 15%;
  border-radius: 20rpx;
  position: fixed; 
}
.cu-prucin{
  height: 100%;
  background-color: rgb(0, 0, 0);
  float: left;
  position: relative;
}
.cu-prucin::after{
  border-radius: 50%;
	background: black;
	position: absolute;
	right: 0rpx;
	content: '';
	display: block;
  margin: calc((6rpx - 14rpx)/2);
	width: 14rpx;
	height: 14rpx;
}
.cu-left{
  position: absolute;
  left: 0;
  margin-top: 16rpx;
  color: gray;
  font-size: 22rpx;
}
.cu-right{
  position: absolute;
  right: 0;
  margin-top: 16rpx;
  color: gray;
  font-size: 22rpx;
}
